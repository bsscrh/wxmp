.container{
    display: flex;
    /* flex主轴垂直排列，默认row水平排列 */
    flex-direction: column;
    /* 副轴居中对齐 */
    align-items: center;
}
.logo{
    width: 392rpx;
    height: 96rpx;
    margin-top: 160rpx;
}
.username{
    margin-top: 100rpx;
    font-size: 32rpx;
    font-weight: 700;
}
.btn{
    margin-top: 100rpx;
    border: 1px solid #109D59;
    padding: 10rpx 20rpx;
    border-radius: 5px;
}
.btn-text{
    color: #109D59;
}
.footer{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  text-align: center;
}